msgid "Please choose a file to save to."
msgstr "Vennligst velg en fil å lagre til."

#: src/bug-buddy.c:834
#, c-format
msgid ""
"The file '%s' already exists.\n"
"Overwrite this file?"
msgstr ""
"Filen '%s' eksisterer allerede.\n"
"Overskriv denne filen?"

#: src/bug-buddy.c:934
#, c-format
msgid ""
"Could not open '%s'.\n"
"Please make sure bug-buddy was installed correctly."
msgstr ""
"Kunne ikke åpne '%s'.\n"
"Vennligst forsikre deg om at bug-buddy ble riktig installert."

#: src/bug-buddy.c:1039
msgid ""
"$GNOME_CRASHED_APPNAME is deprecated.\n"
"Please use the --appname command lineargument instead."
msgstr ""
"$GNOME_CRASHED_APPNAME er utgått.\n"
"Vennligst bruk --appname kommandolinjeflagget i stedet."

#: src/bug-buddy.c:1052
msgid ""
"$GNOME_CRASHED_PID is deprecated.\n"
"Please use the --pid command lineargument instead."
msgstr ""
"$GNOME_CRASHED_PID er utgått.\n"
"Vennligst bruk --pid kommandolinjeflagget i stedet."

#: src/bug-buddy.c:1184
#, c-format
msgid ""
"Could not find '%s'.\n"
"Please make sure bug-buddy was installed correctly."
msgstr ""
"Kunne ikke finne '%s'.\n"
"Vennligst forsikre deg om at bug-buddy ble riktig installert."

#: src/gdb-buddy.c:41
#, c-format
msgid "obtaining stack trace... (%d)"
msgstr "henter stack trace... (%d)"

#: src/gdb-buddy.c:115
#, c-format
msgid ""
"Unable to process core file with gdb:\n"
"'%s'"
msgstr ""
"Kunne ikke håndtere core filen med gdb:\n"
"'%s'"

