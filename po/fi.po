#, c-format
msgid "about to run '%s'"
msgstr "juuri ajamassa '%s'"

#: src/bug-buddy.c:650
#, c-format
msgid "Unable to start mail program: '%s'"
msgstr "Ei pysty käynnistämään sähköpostiohjelmaa: '%s'"

