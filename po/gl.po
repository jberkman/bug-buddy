msgid "Please choose a file to save to."
msgstr "Por favor, escolla un ficheiro no que grabar."

#: src/bug-buddy.c:809
#, c-format
msgid ""
"The file '%s' already exists.\n"
"Overwrite this file?"
msgstr ""
"O ficheiro '%s' xa existe.\n"
"¿Sobrescribilo?"

#: src/bug-buddy.c:906
#, c-format
msgid ""
"Could not open '%s'.\n"
"Please make sure bug-buddy was installed correctly."
msgstr ""
"Non se puido abrir '%s'.\n"
"Por favor, asegúrese de que o bug-buddy está ben instalado."

# Xa sei que 'obsoleto' non é o mesmo que "deprecated", pero non
# atopo outra traducción, e 'deprecado' non ten nada que ver
#: src/bug-buddy.c:1010
msgid ""
"$GNOME_CRASHED_APPNAME is deprecated.\n"
"Please use the --appname command lineargument instead."
msgstr ""
"$GNOME_CRASHED_APPNAME é obsoleto.\n"
"Por favor, use o argumento de liña de comandos --appname no seu lugar."

#: src/bug-buddy.c:1023
msgid ""
"$GNOME_CRASHED_PID is deprecated.\n"
"Please use the --pid command lineargument instead."
msgstr ""
"$GNOME_CRASHED_PID é obsoleto.\n"
"Por favor, use o argumento de liña de comandos --pid no seu lugar."

#: src/bug-buddy.c:1151
#, c-format
msgid ""
"Could not find '%s'.\n"
"Please make sure bug-buddy was installed correctly."
msgstr ""
"Non se puido atopar '%s'.\n"
"Por favor, asegúrese de que o bug-buddy está ben instalado."

