#, c-format
msgid "Unable to open file: '%s'"
msgstr "Non se pode abri-lo ficheiro: '%s'"

#: src/bug-buddy.c:618
#, c-format
msgid "about to run '%s'"
msgstr "a piques de executar '%s'"

#: src/bug-buddy.c:622
#, c-format
msgid "Unable to start mail program: '%s'"
msgstr "Non se pode executa-lo programa de correo: '%s'"

